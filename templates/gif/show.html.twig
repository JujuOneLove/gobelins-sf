{% extends 'base.html.twig' %}

{% block title %}Gif{% endblock %}

{% block body %}
    <h1>Gif</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Link</th>
                <td>{{ gif.link }}</td>
            </tr>
            <tr>
                <th>Tag</th>
                <td>{{ gif.tag }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ gif.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ gif.createdAt ? gif.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ gif.updatedAt ? gif.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DeletedAt</th>
                <td>{{ gif.deletedAt ? gif.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('gif_index') }}">back to list</a>

    <a href="{{ path('gif_edit', {'id': gif.id}) }}">edit</a>

    {{ include('gif/_delete_form.html.twig') }}
{% endblock %}
